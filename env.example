PB_VERSION=0.34.0
PB_HOST=0.0.0.0
PB_PORT=8090

# Optional: bootstrap admin user (recommended for production)
PB_ADMIN_EMAIL=
PB_ADMIN_PASSWORD=

# Optional: 32-character encryption key (use `openssl rand -hex 16`)
ENCRYPTION=

# Optional: container timezone
TZ=UTC+8

# Optional: faster mirrors (override during build/run)
# e.g. https://mirrors.aliyun.com/alpine
ALPINE_MIRROR=
# e.g. https://mirror.ghproxy.com/https://github.com
PB_DOWNLOAD_BASE=
